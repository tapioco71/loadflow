#s(problem-struct :name "test-problem"
                  :maximum-iterations-count 20
                  :frequency 50d0
                  :author "Angelo Rossi"
                  :date 3800100903
                  :network (#s(node-struct :name node-0
                                           :kind :reference)
                            #s(node-struct :name node-1
                                           :kind :generation
                                           :power #c(1d4 1d4)
                                           :voltage 230d0)
                            #s(node-struct :name node-2
                                           :kind :free
                                           :voltage 230d0)
                            #s(node-struct :name node-3
                                           :kind :load
                                           :power #c(2d3 3d2)
                                           :voltage 230d0)
                            #s(bipole-struct :name bipole-1
