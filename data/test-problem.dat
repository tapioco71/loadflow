                                             :nodes (node-0 node-1)
                                             :model-parameters (:value 1d0))
                            #s(bipole-struct :name bipole-2
                                             :kind :admittance
                                             :nodes (node-1 node-2)
                                             :model-parameters (:value 1.2d0))
                            #s(bipole-struct :name bipole-3
                                             :kind :admittance
                                             :nodes (node-2 node-0)
                                             :model-parameters (:value 1d0))
                            #s(bipole-struct :name bipole-4
                                             :kind :admittance
                                             :nodes (node-2 node-0)
                                             :model-parameters (:value 1d0))
                            #s(bipole-struct :name bipole-5
                                             :kind :admittance
                                             :nodes (node-2 node-3)
                                             :model-parameters (:value 1.2d0))
                            #s(bipole-struct :name bipole-6
                                             :kind :admittance
                                             :nodes (node-3 node-0)
                                             :model-parameters (:value 1d0))))
